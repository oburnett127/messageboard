{
  "compilerOptions": {
    "target": "es6",
    "module": "ESNext",
    "jsx": "react",
    "esModuleInterop": true,
    "strict": false,
    "moduleResolution": "node",
    "forceConsistentCasingInFileNames": true,
    "resolveJsonModule": true,
    "skipLibCheck": true
  },
  "include": [
    "src/**/*"
  ]
}
